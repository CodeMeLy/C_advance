        justify-content: center;

        align-items: center;
      }

      .uu-webkit-initial-loading-spinner {
        position: relative;
        width: 226px;
        height: 226px;
      }

      .uu-webkit-initial-loading-outer-spinner {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        border-radius: 50%;
        border: 8px solid transparent;
        border-top-color: #ffffff;
        border-left-color: #ffffff;
        opacity: 0.24;

        animation: spin-revert 2s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
      }

      .uu-webkit-initial-loading-inner-spinner:before {
        content: "";
        position: absolute;
        top: 30px;
        left: 30px;
        right: 30px;
        bottom: 30px;
        border-radius: 50%;
        border: 8px solid transparent;
        border-top-color: #ffffff;
        border-left-color: #ffffff;
        opacity: 0.64;

        animation: spin 1.5s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
      }

      .uu-webkit-initial-loading-inner-spinner:after {
        content: "";
        position: absolute;
        top: 60px;
        left: 60px;
        right: 60px;
        bottom: 60px;
        border-radius: 50%;
        border: 8px solid transparent;
        border-top-color: #ffffff;
        border-left-color: #ffffff;
        opacity: 0.84;

        animation: spin 1.2s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
      }

      @keyframes spin {
        0% {
          transform: rotate(0deg); /* Firefox 16+, IE 10+, Opera */
        }
        100% {
          transform: rotate(360deg); /* Firefox 16+, IE 10+, Opera */
        }
      }

      @keyframes spin-revert {
        0% {
          transform: rotate(0deg); /* Firefox 16+, IE 10+, Opera */
        }
        100% {
          transform: rotate(-360deg); /* Firefox 16+, IE 10+, Opera */
        }
      }

      .uu-webkit-initial-loading-content {
        color: #ffffff;
        font-size: 32px;
        margin-top: 56px;
        font-weight: bold;
        text-align: center;
      }
